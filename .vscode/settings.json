{
	"editor.formatOnSave": true,
	"editor.defaultFormatter": "esbenp.prettier-vscode",
	"editor.codeActionsOnSave": {
		"source.fixAll.stylelint": true
	},
	"stylelint.validate": ["css", "scss"],
	"typescript.tsdk": "node_modules/typescript/lib",
	"search.exclude": {
		"**/node_modules": true
	},
	"files.associations": {
		"commit-msg": "shellscript"
	},
	// Copy all below to your user settings to get nice visual
	"material-icon-theme.files.associations": {
		".inflintignore": "crystal",
		"inflint.config.ts": "crystal",
		"czvinyl.config.cjs": "ocaml",
		".depcheckrc.json": "parcel",
		".cz.json": "jinja",
		"*.routes.ts": "routing",
		".compodocrc.json": "mjml",
		"*.strategy.ts": "nest-gateway"
	},
	"material-icon-theme.folders.associations": {
		".volumes": "database",
		"decorators": "meta",
		"interceptors": "connection",
		"strategies": "mappings",
		"queries": "shader",
		"secrets": "keys",
		"user": "client",
		"_templates": "template"
	},
	"material-icon-theme.activeIconPack": "react_redux",
	"task.allowAutomaticTasks": "on",
	"cSpell.words": ["passowrdvaliteregex", "reduxjs"],
	"reactTsScss.autoComplete": true,
	"reactTsScss.definition": true,
	"reactTsScss.peekProperties": true,
	"reactTsScss.cssAutoComplete": true,
	"reactTsScss.cssDefinitions": true,
	"reactTsScss.diagnostics": true,
	"reactTsScss.baseDir": "src",
	"reactTsScss.cssSyntaxColor": true,
	"reactTsScss.cleanUpDefs": [
		"*.module.css",
		"*.module.scss",
		"*.module.sass",
		"*.module.less",
		"*.module.styl"
	],
	"reactTsScss.references": true,
	"reactTsScss.codelens": false
}
